    "dynasty": "^0.2.7",
    "minimist": "^1.2.0",
    "node-geocoder": "^3.20.0",
    "timezoner": "^0.1.9"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.6.1",
    "eslint-config": "^0.3.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2"
  }
