  "main": "dist/bundle.js",
  "repository": "josephschmitt/echolocate",
  "scripts": {
    "prepare": "rimraf dist/ && rollup --config rollup.config.js"
  },
  "bin": {
    "echolocate": "bin/echolocate.js"
  },
  "keywords": [
    "alexa",
    "ASK",
    "geolocation",
    "time zone"
  ],
